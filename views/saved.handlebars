
{{#each dbArticle}}
    <div class="card">
    <a href="{{link}}">
        <div class="card-header">
            <p class="h3 text-dark">{{title}}</p>
        </div>
    </a> 
    <div class="card-body">
        {{#if summary}}
            <p class="card-text">{{summary}}</p>
        {{/if}}
             <button class="btn btn-dark" data-id={{id}}>Add Note</button>
            <button class="btn btn-warning" data-id={{id}}>Delete</button>
    </div>
    </div>
{{/each}}

