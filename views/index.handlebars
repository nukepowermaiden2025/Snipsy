
<div class="jumbotron-fluid bg">

  <div class="row">
    <div class="col-lg-12">
        {{#each dbArticle}}
            <div class="card" data-id={{id}}>
            <a href="{{link}}"> 
                <div class="card-header">{{title}}</div>
            </a> 
            <div class="card-body">
                <p class="card-text">{{summary}}</p>
                <a href="{{link}}" class="btn btn-primary">Go somewhere</a>
            </div>
            </div>
        {{/each}}
      </div>
    </div>

</div>


    





